    "version": "0.2.0",
    "configurations": [
    
        {
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "name": "Launch FireFox",
            "file": "${workspaceFolder}/Home.html"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome",
            "url": "${workspaceFolder}/Home.html",
            "webRoot": "${workspaceFolder}/Home.html"
        }
        
    ]
}